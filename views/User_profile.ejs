<link rel="stylesheet" type="" href="/css/user_profile.css">
<h1>

    Codial/Profile
    <%= title%>
</h1>

<%if(user.id==profile_user.id){%>
    <form action="/users/update/<%=profile_user.id%>" method="POST">
        <input type="text" name="name" value="<%=profile_user.name%>" required>
        <input type="email" name="email" value="<%=profile_user.email%>" required>
        <button type="submit">Update</button>
    </form>


    <%}else{%>
        <p>Name:
            <%=profile_user.name%>
        </p>
        <p>
            Email:
            <%=profile_user.email%>
        </p>

        <%}%>